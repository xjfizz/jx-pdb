<view class="tabbar_box"><block wx:for="{{tabbar.value.list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['changeTab',['$0'],[[['tabbar.value.list','',index]]]]]]]}}" class="tabbar_box_item" bindtap="__e"><block wx:if="{{item.isSpecial}}"><view class="tabbar_nav" style="{{'color:'+(tabbar.selectedColor)+';'}}" hover-class="none" url="{{item.pagePath}}"><view class="special-wrapper"><image class="{{[item.selected==selected?'tabbar_icon acImg':'tabbar_icon']}}" src="{{item.iconPath}}"></image></view><image class="special-text-wrapper"></image><text>{{item.text}}</text></view></block><block wx:else><view class="tabbar_nav" style="{{'color:'+(item.selected==selected?tabbar.value.selectedColor:tabbar.value.color)+';'}}" hover-class="none" url="{{item.pagePath}}"><image class="{{[item.selected==selected?'tabbar_icon acImg':'tabbar_icon']}}" src="{{item.selected==selected?item.selectedIconPath:item.iconPath}}"></image><text>{{item.text}}</text></view></block></view></block></view>